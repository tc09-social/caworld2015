<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ca.cheyi02.myfirstapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayCoursesActivity"
            android:label="@string/title_activity_display_courses" >
        </activity>
        <activity android:name="com.l7tech.msso.gui.LogonActivity"
            android:theme="@android:style/Theme.Holo.Light.Dialog"
            android:label="Log On to CA World 2015 Demo App"
            android:windowSoftInputMode="stateVisible"
            android:exported="false">
            <intent-filter>
                <action android:name="com.l7tech.msso.service.action.OBTAIN_CREDENTIALS"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <service
            android:name="com.l7tech.msso.service.MssoService"
            android:exported="false" >
        </service>

    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

</manifest>
